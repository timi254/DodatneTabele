public class Naloga2 {
    public static void main(String[] args) {
        char[] tab=new char[15];
        for(int i=0;i<tab.length;i++)
        {
            tab[i]=(char)((Math.random()*(102-97)+97));
        }
        for(int i=0;i<tab.length;i++)
        {
            System.out.print(tab[i]+"  ");
        }
        System.out.println();
        for(int i=0;i<tab.length;i++)
        {
            tab[i]++;
            System.out.print(tab[i]+"  ");
        }

    }
}
